{
  "openapi": "3.1.0",
  "info": {
    "title": "Projeto Prisma",
    "description": "Projeto Prisma com Swagger.",
    "version": "1.0.0",
    "license": {
      "name": "Licença MIT"
    }
  },
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "API de desenvolvimento."
    }
  ],
  "paths": {
    "/contacts": {
      "get": {
        "summary": "Lista contatos",
        "description": "Lista todos os contatos.",
        "tags": ["Contact"],
        "responses": {
          "200": {
            "description": "Sucesso."
          },
          "500": {
            "description": "Erro interno do servidor."
          }
        }
      }
    },
    "/contacts/{contactId}": {
      "get": {
        "summary": "Lista contato",
        "description": "Lista contato pelo seu id.",
        "tags": ["Contact"],
        "parameters": [
          {
            "in": "path",
            "name": "contactId",
            "schema": {
              "type": "string"
            },
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Sucesso."
          },
          "404": {
            "description": "Contato não encontrado."
          },
          "500": {
            "description": "Erro interno do servidor."
          }
        }
      }
    }
  }
}
